module ModernTreasury
  module Type
    module Union
      include ModernTreasury::Type::Converter

      private def self.known_variants: -> ::Array[[Symbol?, (^-> ModernTreasury::Type::Converter::input)]]

      def self.derefed_variants: -> ::Array[[Symbol?, top]]

      def self.variants: -> ::Array[top]

      private def self.discriminator: (Symbol property) -> void

      private def self.variant: (
        Symbol
        | ::Hash[Symbol, top]
        | ^-> ModernTreasury::Type::Converter::input
        | ModernTreasury::Type::Converter::input key,
        ?::Hash[Symbol, top]
        | ^-> ModernTreasury::Type::Converter::input
        | ModernTreasury::Type::Converter::input spec
      ) -> void

      private def self.resolve_variant: (
        top value
      ) -> ModernTreasury::Type::Converter::input?

      def ===: (top other) -> bool

      def ==: (top other) -> bool

      def coerce: (
        top value,
        state: ModernTreasury::Type::Converter::state
      ) -> top

      def dump: (top value) -> top
    end
  end
end
