module ModernTreasury
  module Type
    class HashOf
      include ModernTreasury::Type::Converter

      def self.[]: (
        ::Hash[Symbol, top]
        | ^-> ModernTreasury::Type::Converter::input
        | ModernTreasury::Type::Converter::input type_info,
        ?::Hash[Symbol, top] spec
      ) -> instance

      def ===: (top other) -> bool

      def ==: (top other) -> bool

      def coerce: (
        ::Hash[top, top] | top value,
        state: ModernTreasury::Type::Converter::state
      ) -> (::Hash[Symbol, top] | top)

      def dump: (::Hash[top, top] | top value) -> (::Hash[Symbol, top] | top)

      def item_type: -> top

      def nilable?: -> bool

      def initialize: (
        ::Hash[Symbol, top]
        | ^-> ModernTreasury::Type::Converter::input
        | ModernTreasury::Type::Converter::input type_info,
        ?::Hash[Symbol, top] spec
      ) -> void
    end
  end
end
